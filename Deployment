metadata:
  name: 
  labels:
    app: 
spec:
  replicas: 2
  template:
    metadata:
      name: 
      labels:
        app: 
    spec:
      nodeSelector:
        "beta.kubernetes.io/os": windows
      containers:
      - name: 
        image: strusoftdevopstest.azurecr.io/aspnettest
        resources:
          limits:
